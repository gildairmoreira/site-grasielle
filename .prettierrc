{
  "trailingComma": "none",
  "semi": true,
  "singleQuote": true,
  "endOfLine": "auto",

  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "rangeStart": 0,
  "parser": "typescript",
  "overrides": [
    {
      "files": "*.js *.ts *.jsx *.tsx",
      "options": {
        "parser": "babel"
      }
    }
  ]
}
